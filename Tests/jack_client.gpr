with "../solid";

project Jack_Client is
   for Object_Dir use "obj";
   for Exec_Dir use ".";
   for Source_Files use
      ("jack_client.adb",
       "jack_process.adb");
   for Main use ("jack_client.adb");

   --package Builder renames Solid.Builder;
   -- package Compiler renames Solid.Compiler;
   package Compiler is
      for Default_Switches ("Ada") use ("-fstack-check");
   end Compiler;
   package Builder is
      for Default_Switches ("Ada") use ("-m", "-gnato", "-gnat05", "-O1", "-j2");
   end Builder;
end Jack_Client;
